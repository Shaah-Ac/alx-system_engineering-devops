#!/usr/bin/env bash
# Displays the time for 12 hours and 59 minutes
# Displays hours from 0 to 12
# Displays minutes from 1 to 59

hours=0

while [ $hours -le 12 ]; do
    echo "Hour: $hours"
    for minute in {1..59}; do
        echo $minute
    done
    ((hours++))
done | head -n 71
#!/usr/bin/env bash

hours=0

while [ $hours -le 12 ]; do
  minutes=1
  while [ $minutes -le 59 ]; do
    printf "%02d:%02d\n" $hours $minutes
    ((minutes++))
  done
  ((hours++))
done
